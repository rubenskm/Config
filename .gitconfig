# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Rubens Koji Miura
[alias]
	co = checkout
	cb = checkout -b
	cm = checkout master
	br = branch
	ci = commit
	st = status
	unstage = reset HEAD --
	last = log -1 HEAD
	pr = pull --rebase
	rb = rebase
	psh = push origin HEAD
    undo = reset HEAD~1 --mixed
    amend = commit -a --amend
[pull]
	rebase = true
[merge]
	ff = false
[push]
	default = simple
